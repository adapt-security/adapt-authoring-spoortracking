{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$anchor": "spoorcourse",
  "description": "Adds properties relating to spoor",
  "$merge": {
    "type": "merge",
    "source": { "$ref": "course" },
    "with": {
      "properties": {
        "_latestTrackingId": {
          "type": "number",
